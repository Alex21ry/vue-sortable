{
  "name": "vue-sortable",
  "description": "Easily add drag-and-drop sorting to your Vue.js applications without jQuery, using the v-sortable directive, a thin wrapper for the minimalist RubaXa/SortableJS library.",
  "author": "Jeff Sagal <sagalbot@gmail.com>",
  "version": "0.1.3",
  "main": "vue-sortable.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "karma start karma.conf.js",
    "test-watch": "karma start karma.conf.js --single-run false --auto-watch true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagalbot/vue-sortable.git"
  },
  "dependencies": {
    "sortablejs": "^1.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^3.1.2",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.13",
    "vue": "^2.0.3",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.7.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
